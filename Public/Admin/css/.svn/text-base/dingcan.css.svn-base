@charset "utf-8";
/* CSS Document */
li{ margin:0px; padding:0px; list-style:none;}
.homeBody{}
.homeBody .homeTit{ height:59px; border-bottom:solid 1px #E5E5E5; background:#F9F9F9; padding:0px 110px 0px 20px;}
.homeBody .homeTit h1{ float:left; line-height:59px; font-weight:normal; font-family:"微软雅黑"; font-size:26px; color:#444444; margin:0px;}
.homeBody .homeTit .searchBox{ float:right; padding-top:15px;}
.homeBody .homeTit .input_txt{ margin-bottom:0px;}
.homeBody .homeTit .bn_search{border: 1px solid #D9D9D9; margin-left:-1px; background:#fff; width:58px; height:30px; cursor:pointer; background:#fff url(images/bn_search.gif) no-repeat 14px 2px; vertical-align:top;}
.homeBody .roomInfo{ height:38px; line-height:38px; font-size:16px;}
.homeBody .roomInfo span{ float:right;}
.homeBody .roomInfo span i{ font-style:normal; color:#d30000;}
/*退菜列表*/
.tuicaiList{ width:100%;}
.tuicaiList td{ border-bottom:dashed 1px #D7D7D7; font-size:14px; color:#333; font-family:"宋体"; padding:20px 0px;}
.tuicaiList td.menu_name{ width:790px; padding-left:20px;}
.tuicaiList td.menu_name p{ font-size:12px; margin:0px; color:#A05000; padding-left:22px;}
.tuicaiList td.money{}
.tuicaiList td.liang{ width:130px; text-align:center; }
.tuicaiList td label{ display:inline-block; margin-bottom:0px; vertical-align:middle; padding-left:10px; font-size:14px;}
.tuicaiList td input{ margin-top:0px;}
.tuicaiList input.num{ width:34px; text-align:center; padding:5px 0px; margin:0px 5px;}
.tuicaiList .minus{ border:solid 1px #B9B9B9; width:18px; height:18px; color:#000; text-align:center; line-height:18px; padding:0px; font-size:14px; background:#fff;font-family:"宋体";}
.tuicaiList td.del{ padding-left:35px;}
.tuicaiList td.del label,.tuicaiList td.del span{ text-decoration:line-through;}

.bn_dingcan{ margin-top:20px;}
.bn_dingcan input{ border:none; width:93px; height:36px; font-size:16px; cursor:pointer; color:#fff; text-align:center; margin-left:20px; font-family:"微软雅黑";}
.bn_dingcan input.bn_tuicai{ background:url(images/bn_dingcan.gif) no-repeat;}
.bn_dingcan input.bn_jiesuan{ background:url(images/bn_dingcan.gif) no-repeat 0px -40px;}
.bn_dingcan input.bn_dayin{ background:url(images/bn_dingcan.gif) no-repeat 0px -120px;}
.bn_dingcan input.bn_quxiao{ background:url(images/bn_dingcan.gif) no-repeat 0px -80px;}
/*退菜列表*/
.layerWap{ width:450px; background:#fff; padding:20px 0px; border-radius:10px; position:fixed; left:50%; margin-left:-225px; top:20%; z-index:222222222;}
.layerWap a{ display:block; width:32px; height:32px; background:url(images/bn_close.png) no-repeat; position:absolute; right:-10px; top:-10px;}
.layerWap h2{ text-align:center; font-size:18px; color:#000; font-weight:normal; margin:0px;}
.layerWap .tuidiao_cai{ padding:0px 30px; font-size:14px; line-height:24px; margin-top:10px;}
.layerWap .layer_bn{ margin:0px 20px; border-top:solid 1px #DADADA; text-align:center; padding-top:20px; margin-top:20px;}
.layerWap .layer_bn input{margin:0px 17px;}

/*餐桌列表*/
.canzhuo_list{ margin-top:20px;}
.canzhuo_list li{ float:left; width:135px; height:135px; background:#F3F3F3; text-align:center; margin:0px 1px 1px 0px; color:#454545; cursor:pointer;}
.canzhuo_list li h3{ font-size:18px; font-weight:normal; margin:0px; line-height:inherit; padding-top:30px;}
.canzhuo_list li h4{ font-size:14px; font-weight:normal; margin:4px 0px;}
.canzhuo_list li h5{ font-size:16px; font-weight:normal; margin:0px;}
.canzhuo_list li.guanbi{ background:#B70000; color:#fff;}
.canzhuo_list li h2{ padding-top:10px; line-height:inherit; margin:0px;}
.canzhuo_list li.syz{ background:#5C93EA; color:#fff;}
.canzhuo_list li.syz h3{ padding-top:5px;}
.canzhuo_list li.syz h5{ font-size:14px;}
/*桌号操作*/
.zhuohao_caozuo .layer_bn input{ margin:0px 4px;}
.zhuohao_caozuo .zhuohao_tit{ margin:0px 20px;}
.zhuohao_caozuo .zhuohao_tit h3{ float:left; font-size:18px; font-weight:normal; line-height:30px; margin:0px;}
.zhuohao_caozuo .zhuohao_tit select{ float:right; width:110px; margin:0px;}
.renshu_box{ text-align:center; height:32px; line-height:32px; padding-left:150px; margin-top:20px;}
.renshu_box dt, .renshu_box dd{ float:left;}
.renshu_box dt{ padding-right:5px; line-height:30px; font-size:16px; font-weight:normal;}
.renshu_box .quantity {
border: 1px solid #EAEAEA;
vertical-align: middle;
height: 30px;
display: inline-block;
background-color: white;
}
.renshu_box .quantity .minus {
float: left;
position: relative;
text-align: center;
width: 30px;
height: 30px;
border-right: 1px solid #EAEAEA;
}
.renshu_box .quantity .minus::after {
content: "";
position: absolute;
top: 14px;
left: 8px;
border-top: 2px solid #333;
width: 14px;
}
.renshu_box .quantity .ui-body-c {
float: left;
-webkit-appearance: none;
width: 30px;
height: 30px;
text-align: center;
background: transparent;
border: 0;
vertical-align: top;
font-size: 14px; padding:0px; border-radius:0; margin:0px; background:none;box-shadow:none;
}
.renshu_box .quantity .plus {
float:left;
position: relative;
text-align: center;
display: inline-block;
width: 30px;
height: 30px;
border-left: 1px solid #EAEAEA;
}
.renshu_box .quantity .plus::before {
content: "";
position: absolute;
height: 14px;
border-left: 2px solid #333;
top: 8px;
left: 14px;
}
.renshu_box .quantity .plus::after {
content: "";
position: absolute;
width: 14px;
border-top: 2px solid #333;
top: 14px;
left: 8px;
}
.renshu_box .quantity i {
display: inline-block;
background-repeat: no-repeat;
}


/*结算列表*/
.f_or{ color:#ff5a00;}
.jiesuan_list{ width:100%; border-collapse: collapse; border-spacing: 0; margin: 10px auto 0px; }
.jiesuan_list th{ height:33px; background:#F7F7F7; border-top:solid 1px #E5E5E5;border-bottom:solid 1px #E5E5E5; text-align:center; font-size:14px; color:#000; font-family:"宋体";}
.jiesuan_list th:first-child{ border-left:solid 1px #E5E5E5; text-align:left; padding-left:20px;}
.jiesuan_list th:last-child{ border-right:solid 1px #E5E5E5; }
.jiesuan_list td{ border-bottom:dashed 1px #D7D7D7; text-align:center; padding:15px 0px; font-family:"宋体"; font-size:14px;}
.jiesuan_list tr>td:first-child{ text-align:left; padding-left:10px;}
.jiesuan_list tr>td:last-child{ }
.jiesuan_list input{ vertical-align:middle; margin:0px;}
.jiesuan_list label{ display:inline-block; margin-bottom:0px; line-height:inherit;font-size:14px; vertical-align:middle; }
.jiesuan_list .btn_check{ margin-top:0px; margin-right:10px; vertical-align:middle;}
.jiesuan_list .beizhu{ padding-left:23px; color:#a05000; margin-bottom:0px; font-size:12px;}
.jiesuan_list .xiugai{ display:block; width:45px; height:22px; text-align:center; line-height:22px; color:#fff; font-size:12px; background:#54AA15; border-radius:3px; margin:0 auto;}
.jiesuan_list .queding{ background:#F3582E;}
.jiesuan_list .input_beizhu{ border-color:#fff; padding:0px 8px; margin-top:5px; font-size:12px;}
.jiesuan_list input.num{ width:34px; text-align:center; padding:5px 0px; margin:0px 5px;}
.jiesuan_list .minus{ border:solid 1px #B9B9B9; width:18px; height:18px; color:#000; text-align:center; line-height:18px; padding:0px; font-size:14px; background:#fff;font-family:"宋体";}
.jiesuan_list tr.del>td:first-child{ padding-left:35px;}
.jiesuan_list tr.del td{ text-decoration:line-through;}
.jiesuan_list tr.del label,.jiesuan_list tr.del .beizhu{ padding-left:0px; text-decoration:line-through;}
.jiesuan_list tr.del>td:last-child{ text-decoration:inherit;}
.jiesuan_list tr.del>td:last-child a{ text-decoration:inherit; background:#A3A3A3; cursor:default;}
.heji{ font-size:16px; margin-top:15px; padding-left:35px;}
.jisuanBody .jisuan{ margin-top:15px; padding-left:35px;}
.jisuanBody .jisuan h3{ display:inline-block; font-size:14px; font-family:"宋体"; line-height:30px; margin:0px;}
.jisuanBody .jisuan h3.pd_left{ padding-left:80px;}
.jisuanBody .jisuan .input_money{ width:50px; display:inline-block; margin:0px; font-size:14px; text-align:center; padding:6px 5px;}

/*打印*/
.dayinBody{ width:140px; background:#fff; padding:20px 10px; color:#000; font-size:12px;}
.dayinBody .dayinTit{ padding-bottom:5px; border-bottom:dashed 1px #838383; line-height:20px;}
.dayinBody .dayinTit h3{ font-weight:normal; font-size:14px; margin:0px; padding:0px; line-height:inherit;}
.dayinBody .dayinTit h4{ font-weight:normal; font-size:12px; margin:0px;}
.dayinBody table{ width:100%;border-top:dashed 1px #838383; margin-top:3px;}
.dayinBody th{ padding:10px 0px 5px;}
.dayinBody td{ text-align:center; padding:4px 0px 10px;}
.dayinBody td.name{ text-align:left; padding-bottom:4px;}
.dayinBody .conent{ line-height:18px; padding:10px 0px;border-bottom:dashed 1px #838383;border-top:dashed 1px #838383;}
.dayinBody .conent span{ display:block; line-height:24px; margin:0px;}
.dayinBody .jytime{ line-height:18px; margin-top:5px;}
.dayinBody .jytime span{ display:block; text-align:center; line-height:24px;}